<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<!-- when parallel is none then testng does not consider thread-count
 and run one after another or sequential order
 here thread id will same for all i.e. 1 and run sequentially  
-->
<suite name="Suite" parallel="none" thread-count="2">
	<test name="Test1">
		<classes>
			<class name="parallelExecution.TestClass1" />
		</classes>
	</test>

	<test name="Test2">
		<classes>
			<class name="parallelExecution.TestClass2" />
		</classes>
	</test>

	<test name="Test3">
		<classes>
			<class name="parallelExecution.TestClass3" />
		</classes>
	</test>

	<test name="Test4">
		<classes>
			<class name="parallelExecution.TestClass4" />
		</classes>
	</test>
</suite> <!--
Suite -->
